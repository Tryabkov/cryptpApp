<Window x:Class="crypto.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:vb="clr-namespace:crypto"
        xmlns:svgc="http://sharpvectors.codeplex.com/svgc/"
        mc:Ignorable="d"

         Title="Window1" WindowStartupLocation="CenterScreen" AllowsTransparency="True" MinWidth="1020" MaxWidth="1020" MinHeight="720" MaxHeight="720" WindowStyle="None" ResizeMode="NoResize" SizeToContent="WidthAndHeight">
    <Window.DataContext>
        <vb:MainWindowViewModel/>
    </Window.DataContext>
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary x:Name="Arrow_up" Source="resourses/arrow.xaml"/>
                <ResourceDictionary x:Name="coins" Source="resourses/coins.xaml"/>
            </ResourceDictionary.MergedDictionaries>


            <FontFamily x:Key="Inter1">pack://application:,,,/Fonts/#Noto Sans</FontFamily>

            <!--#region Storyboards-->
            <Storyboard x:Key="OnToggleButtonBackground">
                <ColorAnimation Storyboard.TargetProperty="(Border.Background).(SolidColorBrush.Color)" To="#18191B" Duration="0:0:0.1"/>
            </Storyboard>
            <Storyboard x:Key="OnToggleButtonMargin">
                <ThicknessAnimation Storyboard.TargetProperty="Margin" To="11, 0, 0, 0" Duration="0:0:0.1"/>
            </Storyboard>

            <Storyboard x:Key="OffToggleButtonBackground">
                <ColorAnimation Storyboard.TargetProperty="(Border.Background).(SolidColorBrush.Color)"  To="#E2E7EC" Duration="0:0:0.1"/>
            </Storyboard>
            <Storyboard x:Key="OffToggleButtonMargin">
                <ThicknessAnimation Storyboard.TargetProperty="Margin" To="-11, 0, 0, 0" Duration="0:0:0.1"/>
            </Storyboard>

            <Storyboard x:Key="OnLanguageHeight">
                <DoubleAnimation Storyboard.TargetProperty="Height" From="50" To="284" Duration="0:0:0.1"/>
            </Storyboard>

            <Storyboard x:Key="OffLanguageHeight">
                <DoubleAnimation Storyboard.TargetProperty="Height" To="50" Duration="0:0:0.05" Completed="RemoveLangAnimation"/>

            </Storyboard>
            <!--#endregion-->

            <!--#region Colors-->
            <SolidColorBrush x:Key="Button.Static.Background" Color="#FFDDDDDD"/>
            <SolidColorBrush x:Key="Button.Static.Border" Color="#FF707070"/>
            <SolidColorBrush x:Key="Button.MouseOver.Background" Color="#00BEE6FD"/>
            <SolidColorBrush x:Key="Button.MouseOver.Border" Color="#003C7FB1"/>
            <SolidColorBrush x:Key="Button.Pressed.Background" Color="#00C4E5F6"/>
            <SolidColorBrush x:Key="Button.Pressed.Border" Color="#002C628B"/>
            <SolidColorBrush x:Key="Button.Disabled.Background" Color="#FFF4F4F4"/>
            <SolidColorBrush x:Key="Button.Disabled.Border" Color="#FFADB2B5"/>
            <SolidColorBrush x:Key="Button.Disabled.Foreground" Color="#FF838383"/>
            <SolidColorBrush x:Key="TextBox.Static.Border" Color="#FFABAdB3"/>
            <SolidColorBrush x:Key="TextBox.MouseOver.Border" Color="#007EB4EA"/>
            <SolidColorBrush x:Key="TextBox.Focus.Border" Color="#00569DE5"/>
            <!--#endregion-->

            <Style TargetType="{x:Type Border}">
                <Setter Property="CornerRadius" Value="20"/>
            </Style>

            <Style TargetType="{x:Type TextBlock}">
                <Setter Property="FontWeight" Value="Medium"/>
                <Setter Property="Foreground" Value="#18191B"/>
                <Setter Property="FontFamily" Value="Inter"/>
                <!--<Setter Property="FontFamily" Value=""/>-->
            </Style>

            <!--#region Scrollbar-->
            <Style x:Key="RepeatButtonTransparent" TargetType="{x:Type RepeatButton}">
                <Setter Property="OverridesDefaultStyle" Value="true"/>
                <Setter Property="Background" Value="White"/>
                <Setter Property="Focusable" Value="false"/>
                <Setter Property="IsTabStop" Value="false"/>
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type RepeatButton}">
                            <Border Background="White" Height="{TemplateBinding Height}" Width="{TemplateBinding Width}" CornerRadius="1"/>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>

            <Style x:Key="ScrollBarThumbVertical" TargetType="{x:Type Thumb}">
                <Setter Property="OverridesDefaultStyle" Value="true"/>
                <Setter Property="IsTabStop" Value="false"/>
                <Setter Property="Width" Value="2"/>
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type Thumb}">
                            <Border x:Name="rectangle" Background="#E2E7EC" Width="2" CornerRadius="1"/>
                            <ControlTemplate.Triggers>
                                <Trigger Property="IsMouseOver" Value="true">
                                    <Setter Property="Background" TargetName="rectangle" Value="Black"/>
                                </Trigger>
                                <Trigger Property="IsDragging" Value="true">
                                    <Setter Property="Background" TargetName="rectangle" Value="Black"/>
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>
            <!--#endregion-->

            <!--#region ToolTip Style-->
            <Style x:Key="BaseToolTipStyle" TargetType="{x:Type ToolTip}">
                <Setter Property="Foreground" Value="Transparent" />
                <Setter Property="Background" Value="Transparent" />
                <Setter Property="BorderBrush" Value="Transparent" />
                <Setter Property="BorderBrush" Value="Transparent" />
            </Style>
