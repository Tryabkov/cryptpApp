            <!--#region Rectangle Styles-->
            <Style x:Key="BaseMonthGraph" TargetType="{x:Type Rectangle}">
                <Style.Setters>
                    <Setter Property="Control.Width" Value="6"/>
                    <Setter Property="Control.Height" Value="158"/>
                    <Setter Property="Rectangle.RadiusX" Value="3"/>
                    <Setter Property="Rectangle.RadiusY" Value="3"/>
                    <Setter Property="Rectangle.Fill" Value="#FFE2E7EC"/>
                    <Setter Property="Control.Margin" Value="6.8,0,0,0"/>
                    <Setter Property="Rectangle.VerticalAlignment" Value="Bottom"/>
                </Style.Setters>

                <Style.Triggers>
                    <EventTrigger x:Name="a" RoutedEvent="Rectangle.MouseEnter">
                        <BeginStoryboard>
                            <Storyboard>
                                <ColorAnimation Storyboard.TargetProperty="(Rectangle.Fill).(SolidColorBrush.Color)" To="#18191B" Duration="0:0:0.01"/>
                            </Storyboard>
                        </BeginStoryboard>
                    </EventTrigger>

                    <EventTrigger x:Name="b" RoutedEvent="Rectangle.MouseLeave">

                        <BeginStoryboard>
                            <Storyboard>
                                <ColorAnimation Storyboard.TargetProperty="(Rectangle.Fill).(SolidColorBrush.Color)" To="#FFE2E7EC" Duration="0:0:0.01"/>
                            </Storyboard>
                        </BeginStoryboard>
                    </EventTrigger>
                </Style.Triggers>
            </Style>

